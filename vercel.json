{
	"version": 2,
	"name": "rk4-api",
	"builds": [
		{
			"src": "/src/app.js",
			"use": "@vercel/node"
		},
		{
			"src": "/src/hello_world.py",
			"use": "@vercel/python"
		},
		{
			"src": "/src/script.py",
			"use": "@vercel/python"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/src/app.js"
		}
	]
}
