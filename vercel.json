{
        "version": 2,
        "builds": [
          {
            "src": "src/app.js",
            "use": "@vercel/node"
          },
          {
            "src": "scripts/*.py",
            "use": "@vercel/lambda"
          }
        ],
        "routes": [
          {
            "src": "/api/(.*)",
            "dest": "/src/app.js"
          },
          {
            "src": "/scripts/(.*)",
            "dest": "/scripts/$1.py",
            "methods": ["GET"]
          }
        ]
      }
      